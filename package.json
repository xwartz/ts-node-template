    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.20.0",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
